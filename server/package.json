{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon ./index.js",
    "build": "tsc -p .",
    "knex": "node ./node_modules/knex/bin/cli.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "migrate:seed": "knex migrate:latest && knex seed:run",
    "server": "concurrently -k npm:server:*",
    "server:users": "nodemon ./services/users.js",
    "server:gateway": "wait-on tcp:4001 && nodemon ./index.js"
  },
  "dependencies": {
    "@apollo/federation": "^0.36.1",
    "@apollo/gateway": "0.52.1",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.1.32",
    "graphql-shield": "^7.5.0",
    "helmet": "^6.0.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "wait-on": "^6.0.1"
  }
}
